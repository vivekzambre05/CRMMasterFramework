<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>
	<Properties>
		<Property name="basePath">./src/test/resources/TestData</Property>
	</Properties>
		<!-- report theme -->
		<!-- standard, dark -->
		<theme>DARK</theme>

		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>

		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<protocol>https</protocol>

		<!-- title of the document -->
		<documentTitle>KMB_Extent_Report</documentTitle>

		<!-- report name - displayed at top-nav -->
		<reportName> Sanity Suite 1
		
			<![CDATA[
      			<img src='./src/test/resources/TestData/crmimg.jpg'/>
     		 ]]>
		</reportName>

		<!-- report headline - displayed at top-nav, after reportHeadline -->
		<reportHeadline>Automation Report</reportHeadline>

		 <!-- timestamp format -->
    	<timeStampFormat>MMM dd, yyyy HH:mm:ss</timeStampFormat>

		<!-- custom javascript -->
		<scripts>
			<![CDATA[
                $(document).ready(function() {
                    
                });
            ]]>
		</scripts>

		<!-- custom styles -->
		<styles>
			<![CDATA[
                
            ]]>
		</styles>
	</configuration>
</extentreports>